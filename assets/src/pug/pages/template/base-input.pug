-var sidebar    = true;
include ../../components/mixin

doctype html
html(lang='en')
    include ../../components/header-files
    body
        // page-wrapper Start
        #pageWrapper.page-wrapper.compact-wrapper
            include ../../components/header/header
            // Page Body Start
            .page-body-wrapper
                include ../../components/sidebar
                .page-body
                    .container-fluid
                        .page-title
                            .row
                                .col-sm-6.col-12
                                    h2
                                        | Base Inputs
                                .col-sm-6.col-12
                                    ol.breadcrumb
                                        li.breadcrumb-item
                                            a(href='index.html')
                                                i.iconly-Home.icli
                                        li.breadcrumb-item Form Controls
                                        li.breadcrumb-item.active Base Inputs
                    // Container-fluid starts  
                    .container-fluid
                        .row
                            .col-md-6
                                .card
                                    .card-header
                                        h3 Simple Example
                                        p.mt-1.mb-0 
                                            | Use the
                                            code .form-label 
                                            | and
                                            code .form-control 
                                            | through create basic form.
                                    .card-body.basic-form
                                        .card-wrapper.border.rounded-3
                                            form.row
                                                .mb-3
                                                    label.form-label(for='exampleFormControlInput1') Email address
                                                    input#exampleFormControlInput1.form-control(type='email' placeholder='name@example.com')
                                                div
                                                    label.form-label(for='exampleFormControlTextarea1') Example textarea
                                                    textarea#exampleFormControlTextarea1.form-control(rows='3')
                            .col-md-6
                                .card
                                    .card-header
                                        h3 Sizing
                                        p.mt-1.mb-0
                                            | Use the 
                                            code .form-floating
                                            |  through create floating form.
                                    .card-body.basic-form 
                                        .card-wrapper.border.rounded-3
                                            form.row.floating-wrapper
                                                .mb-3
                                                    input.form-control.form-control-lg(type='text' placeholder='.form-control-lg' aria-label='.form-control-lg example')
                                                .mb-3
                                                    input.form-control(type='text' placeholder='Default input' aria-label='default input example')
                                                .mb-3
                                                    input.form-control.form-control-sm(type='text' placeholder='.form-control-sm' aria-label='.form-control-sm example')
                                                #passwordHelpBlock.form-text
                                                    | Your password must be 8-20 characters long, contain letters and numbers.
                            .col-md-6
                                .card
                                    .card-header
                                        h3 Basic Input Type Control 
                                        p.mt-1.mb-0 
                                            code .form-select
                                            |  to trigger the custom styles.
                                    .card-body.basic-form
                                        form.theme-form.row.g-3
                                            .col-sm-3
                                                label.form-label Place holder
                                            .col-sm-9
                                                input.form-control(type='text' placeholder="This is for hint only ..!")
                                            .col-sm-3
                                                label.form-label Number
                                            .col-sm-9
                                                input.form-control(type='number' value="50")
                                            .col-sm-3
                                                label.form-label Phone Number
                                            .col-sm-9
                                                input.form-control(type="tel" value="91-(999)-999-999")
                                            .col-sm-3
                                                label.form-label URL
                                            .col-sm-9
                                                input.form-control(type="url" value="https://themeforest.net")
                                            .col-sm-3
                                                label.form-label Date and time
                                            .col-sm-9
                                                input.form-control(type="datetime-local" value="birthday" min="2023-05-01")
                                            .col-sm-3
                                                label.form-label Date
                                            .col-sm-9
                                                input.form-control(type="date" value="2023-05-01")
                                            .col-sm-3
                                                label.form-label Time
                                            .col-sm-9
                                                input.form-control(type="time" value="00:00")
                                            .col-sm-3
                                                label.form-label File
                                            .col-sm-9
                                                input.form-control(type="file")
                                            .col-sm-3
                                                label.form-label Maximum Length
                                            .col-sm-9
                                                input.form-control(type="text" maxlength="6" value="Content must be in 6 characters" )
                                            .col-sm-3
                                                label.form-label Color
                                            .col-sm-9
                                                input.form-control.form-control-color(type='color' value="#43B9B2")
                                            .col-sm-3
                                                label.form-label Read only
                                            .col-sm-9
                                                input#staticEmail.form-control-plaintext(type='text' readonly='' value='email@example.com')
                                            .col-sm-3
                                                label.form-label Textarea
                                            .col-sm-9
                                                textarea.form-control(rows="5" cols="5" placeholder="Default textarea")
